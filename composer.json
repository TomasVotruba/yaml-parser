{
    "name": "yaml-parser/yaml-parser",
    "require": {
        "php": "^8.1",
        "symfony/yaml": "^6.1",
        "nette/neon": "^3.3",
        "nette/utils": "^3.2"
    },
    "require-dev": {
        "tracy/tracy": "^2.9",
        "phpunit/phpunit": "^9.5",
        "symplify/easy-testing": "^10.2",
        "phpstan/phpstan": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "YAMLParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YAMLParser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpstan": "vendor/bin/phpstan analyse --ansi"
    }
}
